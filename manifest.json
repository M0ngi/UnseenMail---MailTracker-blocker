{
	"manifest_version": 2,

	"name": "UnseenMail - Mailtrack Blocker",
	"description": "UnseenMail, an extension that blocks email trackers.",
	"version": "1.0",
	"background": {
		"scripts": ["js/background.js"]
	},

	"permissions": [
        "tabs",
        "webRequestBlocking",
        "webRequest",
        "http://*/*",
        "https://*/*",
		"storage"
    ],
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "Block mailtrackers!",
        "default_popup": "html/index.html"
	},
	
	"author": "Mohamed Mongi Saidane - saidanemongi@gmail.com",
	"homepage_url": "https://github.com/M0ngi/UnseenMail-MailTracker-blocker"
}
